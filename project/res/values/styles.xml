        <item name="android:buttonStyle">@style/Button</item>
    </style>
    <style name="DateDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="AppTheme.EventTitleText" parent="@android:style/TextAppearance.DeviceDefault.Medium">
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="EditEvent_Label" parent="@android:style/TextAppearance.DeviceDefault.Medium">
        <item name="android:textColor">@color/edit_event_title_color</item>
