        <item name="android:textSize">30.0sp</item>
    </style>
    <style name="CustomToastText" parent="@android:style/TextAppearance.DeviceDefault.Medium">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/edit_event_title_color</item>
    </style>
