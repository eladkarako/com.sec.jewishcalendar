    <style name="SummaryPrefTextAppearanceWithoutDisable" parent="@android:style/TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">@color/preference_summary_color</item>
    </style>
    <style name="AlertDialogTextAppearance" parent="@android:style/TextAppearance.Holo.Medium" />
    <style name="AlertDialogTitleTextAppearance" parent="@android:style/TextAppearance.Holo.Medium">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Button" parent="@android:style/Widget.Button">
        <item name="android:textSize">22.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">-1.0</item>
        <item name="android:shadowRadius">0.2</item>
    </style>
    <style name="CustomButton" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:buttonStyle">@style/Button</item>
