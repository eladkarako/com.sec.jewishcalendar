        <item name="android:gravity">center</item>
        <item name="android:layout_width">@dimen/next_prev_year_monthe_width</item>
        <item name="android:layout_height">@dimen/next_prev_year_monthe_height</item>
        <item name="android:scaleType">fitXY</item>
